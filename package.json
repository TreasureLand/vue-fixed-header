{
  "name": "vue-fixed-header",
  "version": "0.2.0",
  "description": "Simple and cross-browser friendly fixed header component for Vue.js.",
  "license": "MIT",
  "repository": {
    "url": "potato4d/vue-fixed-header",
    "type": "git"
  },
  "main": "dist/vue-fixed-header.common.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "test": "exit 0",
    "build": "bili",
    "example": "poi",
    "build:example": "poi build"
  },
  "author": {
    "name": "potato4d",
    "email": "mail@potato4d.me"
  },
  "license": "MIT",
  "poi": {
    "entry": "example/index.js",
    "dist": "example/dist",
    "homepage": "./"
  },
  "bili": {
    "format": [
      "cjs",
      "umd"
    ],
    "plugins": [
      "vue"
    ],
    "vue": {
      "css": "dist/vue-fixed-header.css"
    }
  },
  "devDependencies": {
    "bili": "^0.19.0",
    "bulma": "^0.6.1",
    "poi": "^9.3.10",
    "rollup-plugin-vue": "^2.5.2"
  }
}
