{
  "name": "vue-fixed-header",
  "version": "2.0.2",
  "description": "Simple and cross-browser friendly fixed header component for Vue.js.",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue-plugin",
    "vue-component",
    "typescript"
  ],
  "repository": {
    "url": "potato4d/vue-fixed-header",
    "type": "git"
  },
  "main": "dist/vue-fixed-header.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "yarn serve",
    "serve": "vue-cli-service serve ./src/example.ts",
    "build": "vue-cli-service build --target lib src/components/vue-fixed-header.vue",
    "format": "prettier './{src,example}/**/*.{js,ts,vue}' --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "gh-pages": "gh-pages -d docs -m '[ci skip] deploy'"
  },
  "author": {
    "name": "potato4d",
    "email": "mail@potato4d.me"
  },
  "devDependencies": {
    "@types/jest": "24.0.9",
    "@types/lodash.clonedeep": "^4.5.5",
    "@types/node": "10.12.27",
    "@vue/cli": "^3.0.3",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-typescript": "^3.4.0",
    "@vue/cli-plugin-unit-jest": "^3.0.3",
    "@vue/cli-service": "^3.4.0",
    "@vue/component-compiler-utils": "2.6.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "all-contributors-cli": "6.1.2",
    "bulma": "0.7.4",
    "codecov": "3.2.0",
    "gh-pages": "2.0.1",
    "jest": "24.1.0",
    "jsdom": "13.2.0",
    "jsdom-global": "3.0.2",
    "standard-version": "5.0.0",
    "ts-jest": "24.0.0",
    "typescript": "3.0.1",
    "vue-template-compiler": "2.6.7"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "vue": "^2.5.17",
    "vue-property-decorator": "^7.3.0"
